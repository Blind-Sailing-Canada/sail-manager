{
  "author": "it@blindsailing.ca",
  "description": "api for sail manager",
  "license": "MIT",
  "name": "sail-manager-api",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "spellcheck": "cspell 'src/**/*.*'",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ./src/db/ormconfig.ts",
    "typeorm:prod": "ts-node ./node_modules/typeorm/cli.js -d ./db/ormconfig.js",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:show": "yarn typeorm migration:show",
    "migration:show:prod": "yarn typeorm:prod migration:show",
    "migration:run": "yarn typeorm migration:run",
    "migration:run:prod": "yarn typeorm:prod migration:run",
    "migration:revert:prod": "yarn typeorm:prod migration:revert",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:create": "typeorm-ts-node-commonjs migration:create"
  },
  "dependencies": {
    "@google-cloud/storage": "5.14.4",
