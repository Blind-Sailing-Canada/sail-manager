    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.1.2",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "@sentry/node": "7.1.1",
    "@sentry/tracing": "7.1.1",
    "bull": "4.8.3",
    "class-transformer": "0.5.1",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "dotenv": "16.0.1",
    "express-session": "1.17.3",
    "firebase-admin": "9.12.0",
